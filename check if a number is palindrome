#include <iostream>
#include <cmath>
using namespace std;

int main(){
    int num,copy,c=0,result=0,rem;
    cout<<"Enter a number: ";
    cin>>num;
    copy=num;
    while(num!=0){
        c++;
        num/=10;
    }
    num=copy;
    while(num!=0){
        rem=num%10;
        c--;
        result=result+rem*pow(10,c);
        num/=10;
    }
    if(copy==result){
        cout<<copy<<" is palindrome";
    }
    else{
        cout<<copy<<" is not a palindrome";
    }
    return 0;
}
