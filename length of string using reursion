#include <iostream>
#include <string>
using namespace std;

int length(string s1){
    if(s1==""){
        return 0;
    }
    return 1+length(s1.substr(1));
}
int main(){
    string str;
    cout<<"Enter string: ";
    cin>>str;
    int result=length(str);
    cout<<"Length of "<<str<<" is "<<result;
    return 0;
}
