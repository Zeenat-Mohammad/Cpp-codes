#include <iostream>
using namespace std;

int main(){
    int row,col;
    cout<<"Enter no. of rows: ";
    cin>>row;
    cout<<"Enter no. of col: ";
    cin>>col;
    int mat[row][col];
    cout<<"Enter elements of matrix:\n";
    for(int i=0;i<row;i++){
        for(int j=0;j<col;j++){
            cin>>mat[i][j];
        }
    }
    cout<<"Matrix:\n";
    for(int i=0;i<row;i++){
        for(int j=0;j<col;j++){
            cout<<mat[i][j]<<" ";
        }
        cout<<endl;
    }
    bool flag=false;
    if(row==col){
        for(int i=0;i<row;i++){
            for(int j=0;j<col;j++){
                if(mat[i][j]!=mat[j][i]){
                   flag=false;
                   break;
                }
                else{
                    flag=true;
                }
            }
            if(flag==false){
                break;
            }
        }
        if(flag==true){
            cout<<"Matrix is symmertic";
        }
        else{
            cout<<"Matrix is not symmertic";
        }
    }
    return 0;
}
