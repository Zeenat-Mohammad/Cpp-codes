// Write a program to remove all spaces from a string

#include <iostream>
#include <string>
using namespace std;

int main(){
    string s1, s2;
    cout<<"Enter first string: ";
    getline(cin, s1);
    int n=0;
    for (int i=0;i<=s1.size();i++){
       if(s1[i]==' '){
           continue;
       }
       else{
           s2[n++]=s1[i];
       }
    }
    for(int i=0;i<n;i++){
        cout<<s2[i];
    }
    return 0;
}
