#include <iostream>
using namespace std;

int main(){
    int n,count=0;
    cout<<"Enter a number: ";
    cin>>n;
    if(n==1){
        cout<<"Neither prime nor composite";
    }
    for(int i=2;i<=n;i++){
        for(int j=1;j<=n;j++){
            if(i%j==0){
                count++;
            }
        }
        if(count<=2){
            cout<<i<<" ";
        }
        count=0;
    }
    return 0;
}

