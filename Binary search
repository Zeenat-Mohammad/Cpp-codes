#include <iostream>
#include <algorithm>
using namespace std;

int binary_search(int a[],int n,int key){
  int low=0;
  int high=n-1;
  while(low<=high){
    int mid=low+(high-low)/2;
    if(key==a[mid]){
     return mid;
    }
    if(a[mid]<key){
      low=mid+1;
    }
    else{
      high=mid-1;
    }
  }
  return -1;
}

int main() 
{
  int n;
  cout<<"Enter size of array: ";
  cin>>n;
  int a[n], key;
  cout<<"Enter elements of array:\n";
  for(int i=0;i<n;i++){
    cin>>a[i];
  }
  sort(a,a+n);
  cout<<"Enter an element to search in an array:\n";
  cin>>key;
  int res=binary_search(a,n,key);
  if(res==-1){
    cout<<"Not Found";
  }
  else{
    cout<<"Found";
  }
  return 0;
}
