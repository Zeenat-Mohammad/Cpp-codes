// Write a program to count the frequency of a given character in a string.

#include <iostream>
#include <string>
using namespace std;

int main(){
    string s1;
    cout<<"Enter a string: ";
    getline(cin,s1);
    int f=0;
    for(int i=0;i<s1.size();i++){
        for(int j=0;j<s1.size();j++){
            if(s1[i]==s1[j]){
                f++;
            }
        }
        cout<<"frequency of "<<s1[i]<<" "<<f<<endl;
        f=0;
    }
    return 0;
}
