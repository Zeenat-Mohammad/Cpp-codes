#include <iostream>
using namespace std;

int reverse(int arr[],int size){
    if(size==0){
        return 0;
    }
    cout<<arr[size-1]<<" ";
    return reverse(arr,size-1);
}
int main(){
    int n;
    cout<<"Enter size of array: ";
    cin>>n;
    int arr[n];
    cout<<"Enter elements of array: ";
    for(int i=0;i<n;i++){
        cin>>arr[i];
    }
    reverse(arr,n);
    return 0;
}
