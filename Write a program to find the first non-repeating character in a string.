// Write a program to find the first non-repeating character in a string.

#include <iostream>
#include <string>
using namespace std;

int main(){
    string s1;
    cout<<"Enter a string: ";
    getline(cin,s1);
    int freq=0;
    for(int i=0;i<s1.size();i++){
        for(int j=0;j<s1.size();j++){
            if(s1[i]==s1[j]){
                freq++;
            }
        }
        cout<<freq<<endl;
        if(freq==1){
            cout<<s1[i]<<" is the first non repreating character\n";
            break;
        }
        else{
            freq=0;
            continue;
        }
    }
    
    return 0;
}
