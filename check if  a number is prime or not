#include <iostream>
using namespace std;

int main(){
    int num,count=0;
    cout<<"Enter a number: ";
    cin>>num;
    if (num==1){
        cout<<num<<" is neither prime nor composite\n";
    }
    for(int i=2;i<num;i++){
        if(num%i==0){
           count++;
        }
    }
    if(count<2){
        cout<<num<<" is a prime number";
    }
    else{
        cout<<num<<" is not a prime number";
    }
    return 0;
}
